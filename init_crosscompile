
PJDIR=/home/bssn/pjproject
BUILDROOT_DIR=/home/bssn/labs/buildroot


PATH="$BUILDROOT_DIR/output/host/bin:$BUILDROOT_DIR/output/host/sbin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin" 
AR="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-ar" 
AS="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-as" 
LD="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-ld" 
NM="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-nm" 
CC="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-gcc" 
GCC="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-gcc" 
CPP="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-cpp" 
CXX="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-g++" 
FC="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-gfortran" 
F77="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-gfortran" 
RANLIB="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-ranlib" 
READELF="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-readelf" 
STRIP="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-strip" 
OBJCOPY="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-objcopy" 
OBJDUMP="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-objdump" AR_FOR_BUILD="/usr/bin/ar" 
AS_FOR_BUILD="/usr/bin/as" 
CC_FOR_BUILD="/usr/bin/gcc" 
GCC_FOR_BUILD="/usr/bin/gcc" 
CXX_FOR_BUILD="/usr/bin/g++" 
LD_FOR_BUILD="/usr/bin/ld" 
CPPFLAGS_FOR_BUILD="-I$BUILDROOT_DIR/output/host/include" 
CFLAGS_FOR_BUILD="-O2 -I$BUILDROOT_DIR/output/host/include -I/usr/lib/gcc/i686-linux-gnu/6/include/" 
CXXFLAGS_FOR_BUILD="-O2 -I$BUILDROOT_DIR/output/host/include -I/usr/lib/gcc/i686-linux-gnu/6/include/" 
LDFLAGS_FOR_BUILD="-L$BUILDROOT_DIR/output/host/lib -Wl,-rpath,$BUILDROOT_DIR/output/host/lib" 
FCFLAGS_FOR_BUILD="" 
DEFAULT_ASSEMBLER="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-as" DEFAULT_LINKER="$BUILDROOT_DIR/output/host/bin/arm-linux-gnueabihf-ld" CPPFLAGS="-D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -D_FILE_OFFSET_BITS=64" 
CFLAGS="-D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -D_FILE_OFFSET_BITS=64  -Os " 
CXXFLAGS="-D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -D_FILE_OFFSET_BITS=64  -Os " LDFLAGS="" FCFLAGS=" -Os " FFLAGS=" -Os " 
PKG_CONFIG="$BUILDROOT_DIR/output/host/bin/pkg-config"
